{
  "version": 2,
  "projects": {
    "haroldhill": {
      "root": "apps/haroldhill",
      "sourceRoot": "apps/haroldhill",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/haroldhill", "outputPath": "dist/apps/haroldhill" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "haroldhill:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "haroldhill:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "acme:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/acme"],
          "options": {
            "jestConfig": "apps/acme/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/acme/**/*.{ts,tsx,js,jsx}"] }
        }
      }
    },
    "haroldhill-e2e": {
      "root": "apps/haroldhill-e2e",
      "sourceRoot": "apps/haroldhill-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/haroldhill-e2e/cypress.json",
            "tsConfig": "apps/haroldhill-e2e/tsconfig.e2e.json",
            "devServerTarget": "haroldhill:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "haroldhill:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/haroldhill-e2e/**/*.{js,ts}"] }
        }
      }
    }
  }
}
