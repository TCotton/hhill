{
  "version": 2,
  "projects": {
    "contenful": {
      "root": "libs/contenful",
      "sourceRoot": "libs/contenful/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "build": {
          "executor": "@nrwl/web:rollup",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/contenful",
            "tsConfig": "libs/contenful/tsconfig.lib.json",
            "project": "libs/contenful/package.json",
            "entryFile": "libs/contenful/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "compiler": "babel",
            "assets": [
              {
                "glob": "libs/contenful/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/contenful/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/contenful"],
          "options": {
            "jestConfig": "libs/contenful/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "haroldhill": {
      "root": "apps/haroldhill",
      "sourceRoot": "apps/haroldhill",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/haroldhill",
            "outputPath": "dist/apps/haroldhill"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "haroldhill:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "haroldhill:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "haroldhill:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/haroldhill"],
          "options": {
            "jestConfig": "apps/haroldhill/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/haroldhill/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "haroldhill-e2e": {
      "root": "apps/haroldhill-e2e",
      "sourceRoot": "apps/haroldhill-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/haroldhill-e2e/cypress.json",
            "tsConfig": "apps/haroldhill-e2e/tsconfig.e2e.json",
            "devServerTarget": "haroldhill:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "haroldhill:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/haroldhill-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "markdown": {
      "root": "libs/markdown",
      "sourceRoot": "libs/markdown/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/markdown",
            "main": "libs/markdown/src/index.ts",
            "tsConfig": "libs/markdown/tsconfig.lib.json",
            "assets": ["libs/markdown/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/markdown/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/markdown"],
          "options": {
            "jestConfig": "libs/markdown/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "build": {
          "executor": "@nrwl/web:rollup",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/types",
            "tsConfig": "libs/types/tsconfig.lib.json",
            "project": "libs/types/package.json",
            "entryFile": "libs/types/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "compiler": "babel",
            "assets": [
              {
                "glob": "libs/types/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/types/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/types"],
          "options": {
            "jestConfig": "libs/types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
